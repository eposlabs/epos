{
  "$schema": "https://epos.dev/schema.json",
  "name": "Preset Project",
  "alias": "preset",
  "assets": ["./public/logo.svg"],
  "targets": [
    {
      "matches": ["<popup>", "https://epos.dev/@preset"],
      "load": ["./dist/app.js", "./dist/app.css"]
    },
    {
      "matches": "<background>",
      "load": ["./dist/background.js"]
    }
  ]
}
