{
  "$schema": "https://epos.dev/schema.json",

  "name": "web-fs-target",
  "assets": ["public/image.jpg"],
  "targets": [
    {
      "matches": ["*://*/*"],
      "load": ["./src/fs.js", "./src/fs.css"
    },
    {
      "matches": ["https://x.com/*"],
      "load": ["./x2.js", "./x2.css"]
    },
    {
      "matches": "<background>",
      "load": "web-background.js"
    },
    {
      "matches": ["<popup>", "<sidePanel>"],
      "load": "side-panel.js"
    },
    {
      "matches": "frame:https://inssist.com/*",
      "load": "frame.js"
    }
  ],

  "manifest": {
    "name": "Web FS Target",
    "version": "1.0.0",
    "manifest_version": 3
  }
}
