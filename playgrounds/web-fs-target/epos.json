{
  "name": "web-fs-target",
  "assets": ["public/image.jpg"],
  "targets": [
    {
      "matches": ["*://*/*"],
      "load": ["./src/fs.js", "./src/fs.css"]
    },
    {
      "matches": ["https://x.com/*"],
      "load": ["./x2.js", "./x2.css"]
    },
    {
      "matches": "<background>",
      "load": "web-background.js"
    },
    {
      "matches": "frame:https://inssist.com/*",
      "load": "frame.js"
    }
  ]
}
