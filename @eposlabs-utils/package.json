{
  "name": "@eposlabs/utils",
  "version": "1.10.0",
  "type": "module",
  "license": "MIT",
  "author": "imkost",
  "description": "",
  "scripts": {
    "dev": "tsup --config ../tsup.config.ts --watch",
    "build": "tsup --config ../tsup.config.ts",
    "lint": "tsc --noEmit",
    "release": "sh -c 'npm version ${1:-minor} && npm run build && npm publish' --"
  },
  "exports": {
    ".": {
      "source": "./src/utils.ts",
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./types": {
      "source": "./src/utils-types.ts",
      "import": "./dist/utils-types.js",
      "types": "./dist/utils-types.d.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
