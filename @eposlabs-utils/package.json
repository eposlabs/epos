{
  "name": "@eposlabs/utils",
  "version": "1.3.0",
  "type": "module",
  "license": "MIT",
  "author": "imkost",
  "description": "",
  "scripts": {
    "build": "tsup src --format esm --dts --clean --no-splitting",
    "lint": "tsc --noEmit",
    "release": "sh -c 'npm version ${1:-minor} && npm run build && npm publish' --"
  },
  "exports": {
    ".": {
      "source": "./src/utils.ts",
      "import": "./dist/utils.js"
    },
    "./globals": {
      "source": "./src/globals.ts",
      "import": "./dist/globals.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
